<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8.0.291" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mainsource.Controller">
   <columnConstraints>
      <ColumnConstraints />
      <ColumnConstraints />
      <ColumnConstraints />
      <ColumnConstraints />
      <ColumnConstraints />
      <ColumnConstraints />
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
   <children>
      <TextField fx:id="text_chips_villain" GridPane.columnIndex="1" GridPane.valignment="TOP" />
      <Label text="Villain" GridPane.halignment="RIGHT" />
      <TextField fx:id="text_pot" GridPane.columnIndex="1" GridPane.rowIndex="1" />
      <Button fx:id="button_run" mnemonicParsing="false" onAction="#onClickRunParse" text="Deal" GridPane.halignment="CENTER" GridPane.rowIndex="1" />
      <TextField fx:id="text_chips_hero" GridPane.columnIndex="1" GridPane.rowIndex="2" GridPane.valignment="BOTTOM" />
      <Label text="Hero" GridPane.halignment="RIGHT" GridPane.rowIndex="2" />
      <TextArea fx:id="area_log" prefHeight="100.0" prefWidth="500.0" GridPane.columnSpan="7" GridPane.rowIndex="3" />
      <ImageView fx:id="img_villain1" fitHeight="59.0" fitWidth="42.0" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="2" />
      <ImageView fx:id="img_hero1" fitHeight="59.0" fitWidth="42.0" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="2" GridPane.rowIndex="2" />
      <ImageView fx:id="img_board1" fitHeight="59.0" fitWidth="42.0" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="2" GridPane.rowIndex="1" />
      <ImageView fx:id="img_villain2" fitHeight="59.0" fitWidth="42.0" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="3" />
      <ImageView fx:id="img_hero2" fitHeight="59.0" fitWidth="42.0" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="3" GridPane.rowIndex="2" />
      <ImageView fx:id="img_board2" fitHeight="59.0" fitWidth="42.0" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="3" GridPane.rowIndex="1" />
      <ImageView fx:id="img_board3" fitHeight="59.0" fitWidth="42.0" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="4" GridPane.rowIndex="1" />
      <ImageView fx:id="img_board4" fitHeight="59.0" fitWidth="42.0" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="5" GridPane.rowIndex="1" />
      <ImageView fx:id="img_board5" fitHeight="59.0" fitWidth="42.0" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="6" GridPane.rowIndex="1" />
      <TextField fx:id="text_bets_villain" GridPane.columnIndex="1" GridPane.valignment="BOTTOM" />
      <TextField fx:id="text_bets_hero" GridPane.columnIndex="1" GridPane.rowIndex="2" GridPane.valignment="TOP" />
   </children>
</GridPane>
